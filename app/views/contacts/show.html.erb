<html>
    <h1> WorldChicago Partner Organizations Contacts</h1>
    <table>
        <tr>
            <th>Organization</th>
            <th>Name</th>
            <th>Phone number</th>
            <th>Email</th>
        </tr>

        <% @contacts.each do |contact| %>
            <tr>
                <td><%= contact.organization %></td>
                <td><%= contact.name %></td>
                <td><%= contact.phone_number %></td>
                <td><%= contact.email %></td>
            </tr>
        <% end %>
    </table>
</html>
